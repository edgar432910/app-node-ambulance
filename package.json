{
  "name": "nodejs_owner",
  "version": "3.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf cache",
    "test": "cross-env NODE_ENV=test jest  --coverage --passWithNoTests",
    "test:unit": "cross-env NODE_ENV=test jest test/unit/* --coverage=true --passWithNoTests",
    "test:unique": "cross-env NODE_ENV=test jest --coverage=false  --passWithNoTests",
    "test:integration": "cross-env NODE_ENV=test jest test/integration/* --coverage=false --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:config": "cross-env NODE_ENV=test jest --showConfig",
    "test:clear": "cross-env NODE_ENV=test jest --clearCache",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:coverage:unit": "npm run test:unit -- --coverage",
    "test:coverage:integration": "npm run test:integration -- --coverage",
    "lint:report": "eslint --ext ts src/ -f html -o reports/lint.html",
    "format": "prettier --write src/**/*.ts",
    "format:fix": "eslint --ext ts src --fix",
    "dev": "nodemon src/index.ts",
    "build": "npm run clean && tsc && gulp minify",
    "server": "cross-env NODE_ENV=production node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1401.0",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.10.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^2.3.3",
    "permissions-policy": "^0.6.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-logstash-transport": "^2.0.0",
    "yenv": "^3.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.4",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.194",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-ts-alias": "^2.0.0",
    "gulp-ts-alias-plus": "^1.1.0",
    "gulp-ts-paths": "^0.5.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify-es": "^3.0.0",
    "jest": "^29.6.4",
    "jest-extended": "^4.0.1",
    "jest-html-reporter": "^3.10.2",
    "jest-html-reporters": "^3.1.4",
    "jest-sonar-reporter": "^2.0.0",
